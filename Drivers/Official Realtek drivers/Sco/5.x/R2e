ADAPTER:
	DESCRIPTION="RTL 8019 Ethernet Adapter Driver"
	BUS=
	MEDIA_TYPE=ethernet
	MAX_BD=3
	KEY=BASE_IO
	REQUIRED=IRQ
	ADVANCED=
	SPEED=slow
	NET_BOOT=dynamic
